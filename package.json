{
  "name": "datenguide",
  "version": "1.0.0",
  "description": "Datenguide is your guide to understanding and using German official statistics. We work on an easy-to-use data portal and on learning materials around data published by the German statistics offices.",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "next src",
    "build": "next build src",
    "start": "next start src",
    "lint": "npx eslint . --fix",
    "test": "healthier && prettier --check 'src/**/*.{js,json,css}'",
    "format": "prettier --write 'src/**/*.{js,json,css,yml}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datenguide/datenguide.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/datenguide/datenguide/issues"
  },
  "homepage": "https://github.com/datenguide/datenguide#readme",
  "dependencies": {
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.6.0",
    "@mdx-js/mdx": "^1.5.1",
    "@sindresorhus/slugify": "^0.9.1",
    "@svgr/webpack": "^4.3.3",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-mdx": "^1.2.0",
    "clsx": "^1.0.4",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint-config-prettier": "^6.5.0",
    "flexsearch": "^0.6.32",
    "graphql": "^14.5.8",
    "graphql-hooks": "^4.0.2",
    "graphql-hooks-memcache": "^1.2.4",
    "graphql-hooks-ssr": "^1.1.4",
    "graphql-tag": "^2.10.1",
    "gray-matter": "^4.0.2",
    "humps": "^2.0.1",
    "immer": "^5.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "mdi-material-ui": "^6.8.0",
    "next": "^9.1.3",
    "next-fonts": "^0.19.0",
    "next-images": "^1.2.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-highlight": "^0.12.0",
    "react-highlight-words": "^0.16.0",
    "react-hook-thunk-reducer": "^0.1.1",
    "react-responsive-embed": "^2.1.0",
    "react-select": "^3.0.8",
    "reinspect": "^1.0.2",
    "stringify-object": "^3.3.0",
    "typeface-ibm-plex-sans": "^0.0.75",
    "url-parse": "^1.4.7",
    "use-reducer-thunk": "^1.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.6.0",
    "eslint-config-healthier": "^3.2.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "healthier": "^3.2.0",
    "prettier": "^1.18.2",
    "standard": "^14.3.1",
    "webpack": "^4.41.2"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": false
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "react/prop-types": 0,
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "extends": [
      "standard",
      "standard-react",
      "prettier",
      "prettier/react"
    ]
  }
}
